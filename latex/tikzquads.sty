\NewDocumentCommand{\tikzquads@@fitto}{mmmmmm}{
  \tikzmath{coordinate \Ctmp; real \dx , \dy;% , \Rtmp ;
    \Ctmp{from} = (#1);
    \Ctmp{to} = (#2);
      \dx{tmp} = (\Ctmpx{to} - \Ctmpx{from})/100;
      \dy{tmp} = (\Ctmpy{to} - \Ctmpy{from})/100;
      #5 = 50*sqrt((\dx{tmp})^2 + (\dy{tmp})^2)  + #3;
      #6 = atan2(\dy{tmp},\dx{tmp}) + #4;
  }
}

