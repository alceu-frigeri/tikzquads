			\pgftransformshift{\pgfpoint{#2}{#3}}
      \pgftransformscale{#1}
			\pgfpathmoveto{\pgfpoint{-1}{0}}
			\pgfpathlineto{\pgfpoint{1}{0}}
			\pgfpathmoveto{\pgfpoint{0}{-1}}
			\pgfpathlineto{\pgfpoint{0}{1}}    
  }
}

