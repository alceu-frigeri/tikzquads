	\tikzmath{
		real \x,\y;
    \x{center}=0;
    \y{center}=0;
    \x{left}=-\@@sourceradius;
    \x{right}=\@@sourceradius;
		\y{top}=\@@sourceradius;
		\y{bottom}=-\@@sourceradius;
